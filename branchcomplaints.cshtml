@{
    var db1=Database.Open("complaintsdb");
    var branch=Request["b"];
    var thistype="open";
    var sqlselect="SELECT * FROM complaints1 WHERE department=@0 AND type='open'";
    var data=db1.Query(sqlselect,branch);
}

<!DOCTYPE html>

<html lang="en">
    <head>
        <meta charset="utf-8" />
        <title></title>
        <link rel="stylesheet" type="text/css" href="Stylesheet.css">
    </head>
    <body>
        @RenderPage("navigationbar.cshtml")
        <div id="branch_navigator">
            <a href="branchcomplaints.cshtml?b=IT"><p>IT</p></a>
            <a href="branchcomplaints.cshtml?b=CSE"><p>CSE</p></a> 
            <a href="branchcomplaints.cshtml?b=Civil"><p>Civil</p></a>
            <a href="branchcomplaints.cshtml?b=Mechanical"><p>Mechanical</p></a>
            <a href="branchcomplaints.cshtml?b=EEE"><p>EEE</p></a>
            <a href="branchcomplaints.cshtml?b=ECE"><p>ECE</p></a>
            <a href="branchcomplaints.cshtml?b=S&H"><p>S&H</p></a>        
        </div>
        </div>
        
        <br><br><br>
        <h1 style="color: #000;position: fixed;font-family: 'Myriad Pro light';font-weight: 100;">@branch</h1>
        <div id="complaints_container">
            @foreach(var row in data)
        {
           <div id="complaint_cell2">
                <div id="innercell">
                    <p>@row.usercomplaint</p>
                    <p class="email" id="@row.status">@row.email</p> 
                </div>               
           </div>
        }
       </div>
    </body>
</html>
